{
  "compilerOptions": {
    "module": "NodeNext",
    "target": "ES2020",
    "outDir": "dist",
    "rootDir": ".",
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "jsx": "react",
    "moduleResolution": "NodeNext",
    "resolveJsonModule": true,
  },
   "include": [
    "src/**/*",
    "eslint.config.mjs",
  ],
}