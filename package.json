{
  "name": "azure-function-boilerplate",
  "version": "0.0.1",
  "description": "azure-function-boilerplate",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rimraf dist",
    "prestart": "yarn run clean && yarn run build",
    "start": "func start",
    "test": "echo \"No tests yet...\"",
    "dev": "concurrently \"yarn run watch\" \"yarn run start\""
  },
  "dependencies": {
    "@azure/functions": "^4.6.1"
  },
  "devDependencies": {
    "@innovixx/eslint-config": "^1.3.3",
    "@types/node": "^18.x",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "concurrently": "^8.2.1",
    "eslint": "^7.0.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "rimraf": "^5.0.0",
    "typescript": "^4.0.0"
  },
  "main": "dist/src/functions/**/*.js"
}